{
  "name": "todos",
  "version": "1.0.0",
  "private": true,
  "description": "A todo list based on react and redux",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf static && mkdir static && mkdir static/assets",
    "copy": "cp -rf client/assets static && cp client/index.html static",
    "dev": "node server.js",
    "compile": "npm run clean && npm run copy && webpack --progress --config webpack.config.prod.js",
    "build": "npm run compile && electron main.js",
    "start": "npm run dev"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "electron-prebuilt": "^0.36.0",
    "file-loader": "^0.8.5",
    "postcss-loader": "^0.8.1",
    "react": "^15.0.0",
    "react-date-picker": "^5.3.28",
    "react-dom": "^15.0.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "rucksack-css": "^0.8.5",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.7.1"
  },
  "dependencies": {
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "moment": "^2.17.1",
    "nconf": "^0.8.4",
    "node-notifier": "^4.5.0",
    "react-render-html": "^0.1.4"
  }
}
